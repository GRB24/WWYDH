<% provide(:title, 'All Properties') %>

<h1>Vacants</h1>

<%= will_paginate @vacants, previous_label: "&larr; Prev",
next_label: "Next &rarr;", renderer: BootstrapPagination::Rails %>

<% @vacants.each do |v| %>
  <div class="article">
    <h1 class="article-title"><%= link_to v.fulladdress, v %></h1>
    <p class="article-content"><%= v.blocklot %></p>
  </div>
<% end %>

<%= will_paginate @vacants, previous_label: "&larr; Prev",
next_label: "Next &rarr;", renderer: BootstrapPagination::Rails %>
