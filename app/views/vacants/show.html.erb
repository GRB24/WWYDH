<% provide(:title, @vacant.full_address) %>

<body class="full">
	<div class="container">
		<h1 class="address"><%= @vacant.full_address %></h1>
	</div>
	<div class="container">
		<span class="pic">
			pic here
		</span>
		<span class="info">
		
			<div>Neighborhood: <%= @vacant.neighborhood %></div>
			<div>Owner: <%= @vacant.owner %></div>
			<div>Use: <%= @vacant.use %></div>
			<div><%= @vacant.latitude %></div>
			<div><%= @vacant.longitude %></div>

			<div><%= link_to 'Back', vacants_path %></div>
		</span>

	</div>

  <div class="container">
		
		<h1>Google Maps</h1>
		
  				
  					<%= render 'gmaps' %>
  				
			

	</div>




</body>


